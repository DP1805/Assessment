<?xml version="1.0"?>
<launch>

  <master auto="start"/>

  <arg name="map_file" default="$(find box_robot11)/maps/my_map.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />


  <arg name="custom_amcl_launch_file" default="$(find box_robot11)/launch/includes/amcl/$amcl.launch.xml"/>
  <arg name="initial_pose_x" default="0.0"/> 
  <arg name="initial_pose_y" default="0.0"/> 
  <arg name="initial_pose_a" default="0.0"/>


  <include file="$(find box_robot11)/launch/includes/move_base.launch.xml"> 

  </include>

</launch>

